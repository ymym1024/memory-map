    "@types/google.maps": "^3.58.1",
    "@types/leaflet": "^1.9.20",
    "browser-image-compression": "^2.0.2",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.0",
    "exif-js": "^2.3.0",
    "exifr": "^7.1.3",
    "firebase": "^11.10.0",
    "heic-to": "^1.2.1",
    "heic2any": "^0.0.4",
    "leaflet": "^1.9.4",
    "libheif-js": "^1.19.8",
